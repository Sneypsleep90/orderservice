# Database Configuration
DB_HOST=localhost
DB_PORT=5433
DB_USER=myapp_user
DB_PASSWORD=myapp_password
DB_NAME=myapp_db
DB_SSLMODE=disable

# Kafka Configuration
KAFKA_BROKERS=localhost:9092
KAFKA_TOPIC=orders
KAFKA_GROUP_ID=order-service

# Server Configuration
SERVER_PORT=8081

# Migrations
MIGRATIONS_PATH=./migrations
SKIP_MIGRATIONS=false

# Cache
CACHE_TYPE=lru
CACHE_LRU_SIZE=1000

# Kafka DLQ
KAFKA_DLQ_TOPIC=orders-dlq
